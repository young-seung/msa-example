  "jest": {
    "verbose": false,
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettier",
      "npm run lint:fix",
      "git add ."
    ]
  },
  "scripts": {
    "prettier": "prettier --write --config .prettierrc \"./src/**/*.ts\"",
    "lint:fix": "eslint --fix",
    "test": "jest --clearCache && jest",
    "build": "rimraf dist && nest build",
    "start": "nodemon"
  }
